{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "test",
            "command": "dotnet",
            "type": "shell",
            "group": "test",
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "args": [
                "test",
                "${workspaceFolder}/TAN.Core.6.0.Example.IntegrationTest/TAN.Core.6.0.Example.IntegrationTest.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ]
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/TAN.Core.6.0.Example.GraphQL/TAN.Core.6.0.Example.GraphQL.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/TAN.Core.6.0.Example.GraphQL/TAN.Core.6.0.Example.GraphQL.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/TAN.Core.6.0.Example.GraphQL/TAN.Core.6.0.Example.GraphQL.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}